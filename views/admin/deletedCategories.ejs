<!-- main -->
<main class="main-content-wrapper">
    <div class="container">
        <!-- row -->
        <div class="row mb-8">
            <div class="col-md-12">
                <h2>Deleted Categories</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-xl-12 col-12 mb-5">
                <!-- card -->
                <div class="card h-100 card-lg">
                    <div class="px-6 py-6">
                        <div class="table-responsive">
                            <table class="table table-centered table-hover mb-0 text-nowrap table-borderless table-with-checkbox">
                                <thead class="bg-light">
                                    <tr>
                                        <th>Name</th>
                                        <th>Date</th>
                                        <th></th>
                                    </tr>
                                </thead>
								<tbody>
									<% deletedCategory.forEach(function(category) { %>
										<tr>
											<td><%= category.categoryName %></td>
											<td><%= category.date %></td>
											<td>
												<form action="./recover-category" method="post">
													<input type="hidden" name="categoryId" value="<%= category._id %>">
													<button type="submit" class="btn btn-primary">Recover</button>
												</form>
											</td>
										</tr>
									<% }); %>
								</tbody>
								
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>

